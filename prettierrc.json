{
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "twigPrintWidth": 180,
    "plugins": ["./node_modules/prettier-plugin-twig-melody"],
    "disableLanguages": ["html"],
    "twigMultiTags": [
        "nav,endnav",
        "switch,case,default,endswitch",
        "ifchildren,endifchildren",
        "cache,endcache",
        "js,endjs"
    ]
}
